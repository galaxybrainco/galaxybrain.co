  "bugs": {
    "url": "https://github.com/galaxybrainco/galaxybrain.co/issues"
  },
  "homepage": "https://github.com/galaxybrainco/galaxybrain.co#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "sass": "^1.26.9"
  }
}
